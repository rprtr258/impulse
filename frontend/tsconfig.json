{
  "include": [
    "src/**/*",
    "wailsjs/go/**/*.ts"
  ],
  "compilerOptions": {
    // "baseUrl": ".",
    // "paths": {
    //   "@/*": ["./src/*"]
    // }
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitThis": true
  }
}